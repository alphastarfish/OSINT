{
  "framework": {
    "name": "TYLER Force Multiplier Framework",
    "version": "1.0",
    "classification": "OPERATIONAL",
    "deployment_time": "2025-08-03T21:39:06.631040"
  },
  "security": {
    "encryption_standard": "AES-256-GCM",
    "key_rotation_interval": 3600,
    "max_session_age": 7200,
    "require_2fa": true
  },
  "components": {
    "vibe_dashboard": {
      "host": "127.0.0.1",
      "port": 8050,
      "debug": false,
      "auto_refresh": 5
    },
    "nodelink_hub": {
      "host": "0.0.0.0",
      "port": 8443,
      "ssl_enabled": true,
      "max_connections": 1000
    },
    "ai_engine": {
      "model_type": "local",
      "decision_threshold": 0.85,
      "learning_rate": 0.001
    }
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": "logs/tyler.log",
    "max_bytes": 10485760,
    "backup_count": 5
  }
